    <script>
        function load(){
            return fetch ('https://picsum.photos/seed/picsum/200/300/seed')
            .then (res =>{
            if (res.ok){
                return res.json();
            }
            else {
                return Promise.reject(`Http error: ${res.status}`);
            }
            })
        }

        load().
        then(res => {
            console.log(res);
            console.log("działa");
        })

    </script>

